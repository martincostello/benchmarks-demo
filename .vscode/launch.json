{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch benchmarks",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/DotNetBenchmarks/bin/Release/net10.0/DotNetBenchmarks.dll",
      "args": [ "--configuration", "Release" ],
